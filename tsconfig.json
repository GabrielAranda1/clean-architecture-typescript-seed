{
  "compilerOptions": {
    "target": "ES2023",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "./dist/src",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "useUnknownInCatchVariables": false,
    "include": [
      "src"
    ],
    "exclude": [
      "dist",
      "node_modules"
    ],
    "paths": {
      "@/*": [
        "*"
      ]
    }
  }
}